{
  "name": "nuxt-apollo-hasura",
  "version": "1.0.0",
  "description": "Nuxt Apollo Hasura Postgres recipes",
  "author": "Jesus Garcia Gonzalez",
  "private": true,
  "scripts": {
    "serve": "concurrently \"docker-compose up -d\" \"nuxt\"",
    "start": "nuxt start",
    "build": "nuxt build",
    "generate": "nuxt generate",
    "build:gh-pages": "DEPLOY_ENV=GH_PAGES nuxt build",
    "generate:gh-pages": "DEPLOY_ENV=GH_PAGES nuxt generate",
    "lint": "eslint --fix --ext .js,.vue --ignore-path .gitignore .",
    "deploy": "yarn generate:gh-pages && GIT_DEPLOY_REPO=https://github.com/NLeSC/nuxt-apollo-hasura scripts/deploy.sh"
  },
  "dependencies": {
    "@mdi/js": "^5.2.45",
    "@nuxtjs/apollo": "^4.0.1-rc.1",
    "@nuxtjs/pwa": "^3.0.0-beta.20",
    "add": "^2.0.6",
    "apollo-cache-inmemory": "^1.6.6",
    "cookieparser": "^0.1.0",
    "core-js": "^3.6.5",
    "graphql-tag": "^2.10.3",
    "js-cookie": "^2.2.1",
    "jwt-decode": "^2.2.0",
    "nuxt": "^2.12.2",
    "yarn": "^1.22.4"
  },
  "devDependencies": {
    "@babel/runtime-corejs3": "^7.9.6",
    "@nuxtjs/eslint-config": "^3.0.0",
    "@nuxtjs/eslint-module": "^2.0.0",
    "@nuxtjs/vuetify": "^1.11.2",
    "babel-eslint": "^10.1.0",
    "concurrently": "^5.2.0",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-nuxt": ">=1.0.0",
    "eslint-plugin-prettier": "^3.1.3",
    "hasura-cli": "^1.2.1",
    "prettier": "^2.0.5"
  }
}
